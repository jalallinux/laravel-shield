{
    "name": "jalallinux/laravel-shield",
    "description": "A HTTP basic auth middleware for Laravel",
    "keywords": [
        "jalallinux",
        "laravel-shield",
        "auth",
        "basic",
        "http",
        "laravel",
        "middleware",
        "shield"
    ],
    "homepage": "https://github.com/jalallinux/laravel-shield",
    "license": "MIT",
    "authors": [
        {
            "name": "JalalLinuX",
            "email": "smjjalalzadeh93@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "laravel/framework": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "JalalLinuX\\Shield\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JalalLinuX\\Shield\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vinkla\\Shield\\ShieldServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
